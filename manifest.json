{
  "manifest_version": 3,
  "name": "Tab Clubs",
  "version": "1.0",
  "description": "A simple chrome extension to club similar tabs together to make our life easier. Group tabs by category, custom name, and hostname - smartly.",
  "permissions": [
    "tabs",
    "tabGroups",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "action": {
    "default_title": "Tabs",
    "default_icon": "icons/icon48.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icons/icon48.png"
  }
}